{
    "name": "vipinlalrv/laravel-ably-broadcaster",
    "description": "An Ably broadcaster for Laravel",
    "keywords": [
        "springboardvr",
        "laravel-ably-broadcaster",
        "ably",
        "laravel-broadcaster"
    ],
    "homepage": "https://github.com/vipinlalrv91/vipinlalrv-laravel-ably-broadcaster",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vipinlalrv\\LaravelAblyBroadcaster\\": "src/"
        }
    },
    "authors": [
        {
            "name": "vipinlalrv91",
            "email": "vipinlalrv@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0 || ^8.0",
        "ably/ably-php": "^1.1",
        "illuminate/support": "^8.0 || ^9.0",
        "ext-json" : "*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vipinlalrv\\LaravelAblyBroadcaster\\LaravelAblyBroadcasterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
